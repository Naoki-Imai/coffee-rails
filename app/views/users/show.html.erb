<div class="container">
  <h1>about me</h1>
  <%= image_tag @user.image.url %>
  <h3>ユーザー名</h3>
  <p><%= @user.name %></p>
  <% if @user == current_user %>
    <h3>メールアドレス</h3>
    <p><%= @user.email %></p>
    <%= link_to  "編集", edit_user_registration_path, class: "btn btn-primary" %>
  <% end %>
</div>